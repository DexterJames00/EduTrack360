{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "jsx": "react-jsx",
  "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@types": [
        "src/types/index"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  },
  "include": [
    "index.ts",
    "App.tsx",
    "src/**/*"
  ],
  "extends": "expo/tsconfig.base"
}
